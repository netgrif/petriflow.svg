{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "application-builder": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "nab",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/application-builder",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "aot": false,
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            "src/.htaccess",
                            {
                                "glob": "**/*",
                                "input": "node_modules/ngx-monaco-editor/assets/monaco",
                                "output": "./assets/monaco/"
                            }
                        ],
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
                            "src/netgif-theme.scss",
                            "src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "6mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "application-builder:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "application-builder:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "application-builder:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
                            "src/styles.scss"
                        ],
                        "scripts": [],
                        "codeCoverage": true
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "tsconfig.app.json",
                            "tsconfig.spec.json",
                            "e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "application-builder:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "application-builder:serve:production"
                        }
                    }
                }
            }
        },
        "petriflow-canvas": {
            "projectType": "library",
            "root": "projects/petriflow-canvas",
            "sourceRoot": "projects/petriflow-canvas/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/petriflow-canvas/tsconfig.lib.json",
                        "project": "projects/petriflow-canvas/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/petriflow-canvas/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/petriflow-canvas/src/test.ts",
                        "tsConfig": "projects/petriflow-canvas/tsconfig.spec.json",
                        "karmaConfig": "projects/petriflow-canvas/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/petriflow-canvas/tsconfig.lib.json",
                            "projects/petriflow-canvas/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "canvas": {
            "projectType": "library",
            "root": "projects/canvas",
            "sourceRoot": "projects/canvas/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/canvas/tsconfig.lib.json",
                        "project": "projects/canvas/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/canvas/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/canvas/src/test.ts",
                        "tsConfig": "projects/canvas/tsconfig.spec.json",
                        "karmaConfig": "projects/canvas/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/canvas/tsconfig.lib.json",
                            "projects/canvas/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "application-builder",
    "cli": {
        "analytics": "b689845b-1590-4bc6-a365-06155d598a09"
    }
}
